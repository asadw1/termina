{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MusicFilesPath": "../MusicFiles",
  "MusicProvider": "Local", // Options: Local, Spotify, Pandora
  "SpotifyConfig": {
    "ClientId": "your-spotify-client-id",
    "ClientSecret": "your-spotify-client-secret",
    "RedirectUri": "your-spotify-redirect-uri"
  },
  "PandoraConfig": {
    "ApiKey": "your-pandora-api-key",
    "Secret": "your-pandora-secret"
  }
}